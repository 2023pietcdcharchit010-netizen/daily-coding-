
#include <iostream>
using namespace std;
void swap(int &a,int &b){  //pass by reference
    int c;
    c=a;
    a=b;
    b=c;
}
void swap (float &c,float &d){   // fucntion with name is called fun overloading
    float r;
    r=c;
    c=d;
    d=r;
}
int main()
{
    int a,b;
    cin>>a>>b;
    swap(a,b);
    cout<<a<<" "<<b<<" "<<endl;

    float f1=1.3,f2=1.7;
    swap (f1,f2);
    cout<<f1<<" "<<f2;
}
