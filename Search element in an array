#include <iostream>
using namespace std;

int main()
{
    int arr[6] = {6, 5, 4, 8, 11, 6};
    int x;              
    int index = -1;       
    cout << "Enter element to search: ";
    cin >> x;
    for (int i = 0; i < 6; i++)
    {
        if (arr[i] == x)
        {
            index = i;    
            break;
        }
    }
    cout << index;
    return 0;
}
