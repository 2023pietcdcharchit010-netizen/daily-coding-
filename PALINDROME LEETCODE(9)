class Solution {
public:
    bool isPalindrome(int x) {
        long long ans=0,rem;
        int num=x;
        if(num<0)
        return 0;
        while(num)
        {
            rem=num%10;
            num/=10;
            ans=ans*10+rem;
        }
        if(ans==x)
        return 1;
        else 
        return 0;
    }
};
